<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>phpMyAdmin</Name>
  <Description>phpMyAdmin for unRAID
</Description>
  <Registry>https://hub.docker.com/r/hernandito/phpmyadmin-docker/</Registry>
  <Repository>hernandito/phpmyadmin-docker</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>1962</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/cache/appdata/phpmyadmin/</HostDir>
      <ContainerDir>/sessions</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/cache/appdata/phpmyadmin/www/</HostDir>
      <ContainerDir>/www</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/etc/localtime</HostDir>
      <ContainerDir>/etc/localtime</ContainerDir>
      <Mode>ro</Mode>
    </Volume>
  </Data>
  <Version>7a2623cf</Version>
  <WebUI>http://[IP]:[PORT:1962]/</WebUI>
  <Banner>http://i.imgur.com/gQYUBaz.png</Banner>
  <Icon>https://github.com/hernandito/phpMyAdmin-Docker/raw/master/phpmyadmin.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
