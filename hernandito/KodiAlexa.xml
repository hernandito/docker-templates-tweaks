<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Kodi-Alexa</Name>
  <Beta>False</Beta>
  <Category>MediaServer:Video MediaServer:Music</Category>
  <Description>
    test for Alexa commands to control Kodi [br][br]


  </Description>
  <Registry></Registry>
  <GitHub></GitHub>
  <Repository>kuroshi/kodi-alexa</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>TERM</Name>
      <Value>xterm</Value> 
    </Variable>
    <Variable>
      <Name>KODI_ADDRESS</Name>
      <Value>192.168.0.101</Value> 
    </Variable>
    <Variable>
      <Name>KODI_PORT</Name>
      <Value>80</Value> 
    </Variable>
    <Variable>
      <Name>KODI_USERNAME</Name>
      <Value>xbmc</Value> 
    </Variable>
    <Variable>
      <Name>KODI_PASSWORD</Name>
      <Value>xbmc</Value> 
    </Variable>

	</Environment>
  <Networking>
    <Mode>host</Mode>
     <Publish>
      <Port>
        <HostPort>443</HostPort>
        <ContainerPort>443</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
  </Data>
  <WebUI>http://[IP]:[PORT:9000]/</WebUI>
  <Banner></Banner>
  <Icon>http://imgur.com/download/OGfy3hh</Icon>
</Container>
