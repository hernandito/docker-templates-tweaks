<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>PhotoShow</Name>
  <Description>Photoshow is a web gallery app which can use local files without the need to import them[b][br][br]You must change host port 80 to avoid clash with unraid webui[br][br]Map /Pictures to your unraid photo library and /Thumbs to somewhere on your appdata share</Description>
  <Registry>https://registry.hub.docker.com/u/hernandito/docker-photoshow/</Registry>
  <Repository>hernandito/docker-photoshow</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8668</HostPort>
        <ContainerPort>80</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/appdata/photoshow/pictures/</HostDir>
      <ContainerDir>/Pictures</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/photoshow/thumbs/</HostDir>
      <ContainerDir>/Thumbs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version>a1991205</Version>
  <WebUI>http://[IP]:[PORT:80]/?t=Login</WebUI>
  <Banner></Banner>
  <Icon>http://i.imgur.com/I9ycLHg.png</Icon>
  <ExtraParams></ExtraParams>
</Container>
